{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@juicyfx/vercel-php@2.0.1",
            "config": {
                "php": "8.2"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}